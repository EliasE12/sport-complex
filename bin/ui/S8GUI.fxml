<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.S8Controller">
   <center>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <center>
            <BorderPane prefHeight="200.0" prefWidth="200.0">
               <top>
                  <Label alignment="CENTER" prefHeight="40.0" prefWidth="124.0" text="Information" BorderPane.alignment="CENTER">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </top>
               <center>
                  <TextArea fx:id="taView" prefHeight="326.0" prefWidth="594.0">
                     <BorderPane.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                     </BorderPane.margin>
                     <padding>
                        <Insets bottom="2.0" left="4.0" right="4.0" top="2.0" />
                     </padding>
                  </TextArea>
               </center>
               <bottom>
                  <HBox alignment="CENTER" prefHeight="41.0" prefWidth="600.0" spacing="80.0">
                     <children>
                        <Button fx:id="btImport" mnemonicParsing="false" onAction="#controlBtImport" text="Import" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </Button>
                        <Button fx:id="btExport" mnemonicParsing="false" onAction="#controlBtExport" text="Export" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
               </bottom>
            </BorderPane>
         </center>
      </BorderPane>
   </center>
</BorderPane>
